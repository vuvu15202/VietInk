@page
@model VietInkWebApp.Pages.shoppingcart.IndexModel
@{
    var cart = Model.Cart;
    var total = 0;
    if (cart.CartItems != null)
    {
        foreach(var item in cart.CartItems)
        {
            total += item.Quantity * (int)item.Product.UnitPrice;
        }
    }
}

