@model VietInkWebApp.Pages.contact.IndexModel
@using System.Text.Json;
@using VietInkWebApp.Entities;
@{
}@{
    var cartvalue = 0;
    try
    {
        string jsonData = string.IsNullOrEmpty(Request.Cookies["Cart"]) ? "" : Request.Cookies["Cart"];
        if (!jsonData.Equals(""))
        {
            cartvalue = JsonSerializer.Deserialize<Cart>(jsonData).NumberOfItem;
        }
        else cartvalue = 0;
    }
    catch (Exception ex)
    {
        cartvalue = 0;
    }
}

